{
  "appId": "com.numberfit",
  "appName": "NumberFit",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "server": {
   "allowNavigation": ["primaryapp-env.eba-aitxzvsh.eu-west-2.elasticbeanstalk.com", "*.primaryapp-env.eba-aitxzvsh.eu-west-2.elasticbeanstalk.com"]
  },
  "cordova": {
    "preferences": {
      "ScrollEnabled": "false",
      "android-minSdkVersion": "19",
      "BackupWebStorage": "none",
      "SplashMaintainAspectRatio": "true",
      "FadeSplashScreenDuration": "300",
      "SplashShowOnlyFirstTime": "false",
      "SplashScreen": "screen",
      "SplashScreenDelay": "3000"
    }
  }
}
